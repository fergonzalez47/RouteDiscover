
<h2>Favorites Routes</h2>
{{#if favorites}}
<div class="routes-container">
    {{#each favorites}}
    <div class="route-parent">
        <div class="route-title">
            <h4>{{this.trekkingRoute}}</h4>
        </div>
        <div class="route-parent-detail">
            <a href="/trails/{{this._id}}">See more</a>
        </div>

    </div>
    {{/each}}
    {{/if}}
</div>